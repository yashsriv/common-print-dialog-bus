<?xml version="1.0" encoding="UTF-8"?>

<node name="org/openprinting/PrintDialog/Manager" xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd">

  <interface name="org.openprinting.PrintDialog.Manager">

    <doc:doc>
      <doc:description>
        <doc:para>PrintDialog.Manager is responsible for creating and managing
        PrintDialog objects for multiple applications.</doc:para>
      </doc:description>
    </doc:doc>

    <method name="CreatePrintDialog">
      <doc:doc>
        <doc:description>
          <doc:para>Creates a new print dialog for the calling application.</doc:para>
        </doc:description>
      </doc:doc>
      <arg name="dialog" type="o" direction="out">
        <doc:doc>
          <doc:summary>Object path to the newly created PrintDialog.</doc:summary>
        </doc:doc>
      </arg>
    </method>

  </interface>

  <interface name="org.freedesktop.DBus.Introspectable">

    <doc:doc>
      <doc:description>
        <doc:para>DBus Introspectable interface for other applications.</doc:para>
      </doc:description>
    </doc:doc>

    <method name="Introspect">
      <doc:doc>
        <doc:description>
          <doc:para>Returns Introspection data.</doc:para>
        </doc:description>
      </doc:doc>
      <arg name="xml_data" type="s" direction="out">
        <doc:doc>
          <doc:summary>XML Introspection data.</doc:summary>
        </doc:doc>
      </arg>
    </method>

  </interface>

</node>

